<div id="mo-nav" class="">
  <div class="m-avatar">
    <img src="<%- (theme.cdn || '') + theme.avatar%>">
  </div>
  <p style="text-align: center; color: #333; font-weight: 900; font-family: 'Ubuntu', sans-serif; letter-spacing: 1.5px; margin-bottom: 0.5em;"><%- theme.prefixName%><%- theme.siteName%></p>
  <!-- <p style="text-align: center; word-spacing: 20px;">
    <% if (theme.msocial) {%>
      <% for (var i in theme.msocial) {%>
        <a href="<%- theme.msocial[i].url%>" class="fa <%- theme.msocial[i].fa%>" target="_blank" style="color: #<%- theme.msocial[i].color%>; margin-left:20px"></a>
      <% } %>
    <% } %>
  </p> -->
  <div class="mtop-social_v2">
    <% if (theme.msocial) {%>
      <% for (i in theme.msocial) {%>
          <li>
            <a href="<%= theme.msocial[i].url%>" target="_blank" class="fa <%- theme.msocial[i].fa%>" title="<%- i %>">
              <img src="<%- (theme.cdn || '') + theme.msocial[i].img%>">
              <!-- <a href="<%- theme.social[i].url%>" class="fa <%- theme.social[i].fa%> fa-lg" target="_blank" style="color: #<%- theme.social[i].color%>; margin-left:20px" title="<%- i %>" ></a> -->
            </a>
          </li>
      <% } %>
    <% } %>
  </div>
  <ul id="menu-new-1" class="menu" style="margin: 1.5em 0 1.5em 3em;">
    <% for (var menu in theme.menus){ %>
      <li>
        <a href="<%- url_for(theme.menus[menu].path) %>">
          <span class="faa-parent animated-hover">
            <i class="fa  <%= theme.menus[menu].fa %>" aria-hidden="true"></i>
            <%= menu %>
          </span>
        </a>
        <% if (theme.menus[menu].submenus) { %>
          <ul class="sub-menu">
            <% for (var submenu in theme.menus[menu].submenus){ %>
              <li>
                <a href="<%- url_for(theme.menus[menu].submenus[submenu].path) %>">
                  <i class="fa <%= theme.menus[menu].submenus[submenu].fa %>" aria-hidden="true"></i>
                  <%= submenu %>
                </a>
              </li>
            <% } %>
          </ul>
        <% } %>
      </li>
    <% } %>
  </ul>
  <!-- <p style="text-align: center; font-size: 13px; color: #b9b9b9;">&copy 2019 hexo-sakura</p> -->
</div>
<button onclick="topFunction()" class="mobile-cd-top" id="moblieGoTop" title="Go to top" style="display: none;"><i class="fa fa-chevron-up" aria-hidden="true"></i></button>